{% extends 'base.html.twig' %}

{% block title %}Licence{% endblock %}

{% block body %}
    <h1>Licence</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ licence.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ licence.titre }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ licence.prix }}</td>
            </tr>
            <tr>
                <th>Information</th>
                <td>{{ licence.information }}</td>
            </tr>
            <tr>
                <th>Horaire</th>
                <td>{{ licence.horaire }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_licence_index') }}">back to list</a>

    <a href="{{ path('app_licence_edit', {'id': licence.id}) }}">edit</a>

    {{ include('licence/_delete_form.html.twig') }}
